{
  "name": "@aidbox-forms/renderer",
  "version": "0.0.2-pre-alpha",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HealthSamurai/aidbox-forms-renderer"
  },
  "homepage": "https://healthsamurai.github.io/aidbox-forms-renderer/",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "dev": "vite",
    "build": "tsc -b ./tsconfig.lib.json && vite build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json && tsc --noEmit -p tsconfig.node.json && tsc --noEmit -p tsconfig.storybook.json",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "test": "vitest",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "fhirpath": "^4.6.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "react": ">=18",
    "react-dom": ">=18",
    "@aidbox-forms/nshuk-theme": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@aidbox-forms/nshuk-theme": {
      "optional": true
    }
  },
  "dependencies": {
    "@aidbox-forms/theme": "workspace:*",
    "@aidbox-forms/hs-theme": "workspace:*",
    "@lhncbc/ucum-lhc": "^7.1.3",
    "classnames": "^2.5.1",
    "mobx-utils": "^6.1.1",
    "nanoid": "^5.1.6"
  }
}
