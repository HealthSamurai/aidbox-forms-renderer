{
  "name": "@aidbox-forms/renderer",
  "version": "0.0.3-pre-alpha",
  "description": "Aidbox Forms Renderer for FHIR R5 Questionnaires",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HealthSamurai/aidbox-forms-renderer"
  },
  "homepage": "https://healthsamurai.github.io/aidbox-forms-renderer/",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "dev": "vite",
    "build": "tsc -b ./tsconfig.lib.json && vite build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "test": "vitest",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@aidbox-forms/theme": "workspace:*",
    "@lhncbc/ucum-lhc": "7.1.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/fhir": "^0.0.41",
    "@types/fuzzy-search": "^2.1.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "fhir": "^4.12.0",
    "jsdom": "^27.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@aidbox-forms/hs-theme": "workspace:*",
    "@aidbox-forms/nshuk-theme": "workspace:*",
    "@lhncbc/ucum-lhc": "^7.1.3",
    "classnames": "^2.5.1",
    "fhirpath": "^4.6.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "mobx-utils": "^6.1.1",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependenciesMeta": {
    "@aidbox-forms/nshuk-theme": {
      "optional": true
    }
  },
  "dependencies": {
    "fuzzy-search": "^3.2.1",
    "hashery": "^1.3.0"
  }
}
