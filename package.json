{
  "name": "aidbox-forms-workspace",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "pnpm -r run --if-present build",
    "dev": "pnpm --filter @aidbox-forms/renderer dev",
    "lint": "pnpm -r run --if-present lint",
    "typecheck": "pnpm -r run --if-present typecheck",
    "format": "pnpm -r run --if-present format",
    "test": "pnpm -r run --if-present test",
    "storybook": "pnpm --filter @aidbox-forms/renderer storybook",
    "build-storybook": "pnpm --filter @aidbox-forms/renderer build-storybook",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@codemirror/lang-json": "^6.0.2",
    "@eslint/js": "^9.38.0",
    "@microsoft/api-extractor": "^7.53.3",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/fhir": "^0.0.41",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@uiw/react-codemirror": "^4.25.2",
    "@vitejs/plugin-react": "^5.1.0",
    "codemirror": "^6.0.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.0.8",
    "fhir": "^4.12.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-resizable-panels": "^3.0.6",
    "storybook": "^10.0.8",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-css-modules": "^1.11.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cts,mts,cjs,mjs,json,css,scss,md,mdx,html,yaml,yml}": "prettier --write"
  }
}
