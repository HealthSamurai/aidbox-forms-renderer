{
  "name": "aidbox-forms-workspace",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "prepare": "husky",
    "build": "pnpm -r run --if-present build",
    "dev": "pnpm --filter @aidbox-forms/renderer dev",
    "lint": "pnpm -r run --if-present lint",
    "typecheck": "pnpm -r run --if-present typecheck",
    "format": "pnpm -r run --if-present format",
    "test": "pnpm -r run --if-present test",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.0.8",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cts,mts,cjs,mjs,json,css,scss,md,mdx,html,yaml,yml}": "prettier --write"
  }
}
